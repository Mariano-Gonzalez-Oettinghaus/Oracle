[32m+[m[32m    <script>[m
[32m+[m[32m        // var secreto = Math.round(Math.random()*10);[m
[32m+[m[32m        var secretos = [3,5,7,9];[m
[32m+[m[41m       [m
[32m+[m
[32m+[m
[32m+[m[32m        var input = document.querySelector("input");[m
[32m+[m[32m        input.focus();[m
[32m+[m
[32m+[m[32m        function verificar(){[m
[32m+[m[32m            var encontrado = false;[m
[32m+[m
[32m+[m[32m            for(var posicion = 0; posicion <= secretos.length; posicion++){[m
[32m+[m
[32m+[m[32m                if (parseInt(input.value)  == secretos[posicion]){[m
[32m+[m
[32m+[m[32m                alert("Usted acerto")[m
[32m+[m[32m                encontrado = true;[m
[32m+[m[32m                break;[m
[32m+[m[32m            }[m
[32m+[m
[32m+[m[32m           }[m
[32m+[m[32m                if (encontrado == false){[m
[32m+[m
[32m+[m[32m                    alert("Usted fallo")[m
[32m+[m[32m                    }[m
[32m+[m[41m                    [m
[32m+[m
[32m+[m[41m             [m
[32m+[m
[32m+[m[32m                input.value = "";[m
[32m+[m[32m                input.focus();[m
[32m+[m
[32m+[m[32m            }[m
[32m+[m[41m                [m
[32m+[m[32m                var button = document.querySelector("button");[m
[32m+[m[32m                button.onclick = verificar;[m
[32m+[m
[32m+[m[41m            [m
[32m+[m[41m           [m
[32m+[m[32m    </script>[m
[32m+[m
