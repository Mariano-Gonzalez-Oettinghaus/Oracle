[32m+[m
[32m+[m
[32m+[m[41m    [m
[32m+[m[32m    function alterarColor() {[m
[32m+[m[32m        contador++[m
[32m+[m[32m        if(contador >= colores.length){[m
[32m+[m
[32m+[m[32m            contador = 0[m
[32m+[m
[32m+[m[32m        }[m
[32m+[m[41m        [m
[32m+[m[32m        return false;[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    pantalla.oncontextmenu = alterarColor;[m
[32m+[m
[32m+[m[32m</script>[m[41m [m
