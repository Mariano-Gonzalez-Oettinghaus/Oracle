[32m+[m[32m    function mostrarMensaje(evento){[m
[32m+[m[32m        var x = evento.pageX - pantalla.offsetLeft; //pos x[m
[32m+[m[32m        var y = evento.pageY - pantalla.offsetTop; //pos y[m
[32m+[m[32m        var d = new Date(); //crea una variable de tipo date[m
[32m+[m[32m        var hora = checkTime(d.getHours()) + ":" + checkTime(d.getMinutes()) + ":" + checkTime(d.getSeconds());[m
[32m+[m[32m        //Verifica si el n√∫mero de las horas, minutos o segundos, tiene un solo d√≠gito,[m[41m [m
[32m+[m[32m        //caso positivo le adiciona el cero en la frente para obtener el formato deseado 00:00:00[m
[32m+[m[32m        function checkTime(i){[m
[32m+[m[32m            if (i<10){[m
[32m+[m[32m                i="0" + i;[m
[32m+[m[32m            }[m
[32m+[m[32m            return i;[m
[32m+[m[32m        }[m
[32m+[m
[32m+[m[32m        alert("La hora es: " + hora + " y las coordenadas son: x=" + x + ", y=" + y);[m
[32m+[m[32m    }[m
[32m+[m[32m        pantalla.onclick = mostrarMensaje[m
[32m+[m
[32m+[m[32m    // Aqu√≠ viene la llamada a la funci√≥n con el evento onclick[m
[32m+[m
[32m+[m[32m</script>[m[41m [m
