[36m@@ -0,0 +1,46 @@[m
[32m+[m[32m<canvas width="600" height="400"></canvas>[m
[32m+[m
[32m+[m[32m<script>[m
[32m+[m
[32m+[m[32m    function dibujarRectangulo(x, y, base, altura, color) {[m
[32m+[m[32m        var pantalla = document.querySelector("canvas");[m
[32m+[m[32m        var pincel = pantalla.getContext("2d");[m
[32m+[m
[32m+[m[32m        pincel.fillStyle=color;[m
[32m+[m[32m        pincel.fillRect(x,y, base, altura);[m
[32m+[m[32m        pincel.strokeStyle="black";[m
[32m+[m[32m        pincel.strokeRect(x,y, base, altura);[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function escribirTexto(x , y, texto) {[m
[32m+[m[32m        var pantalla = document.querySelector("canvas");[m
[32m+[m[32m        var pincel = pantalla.getContext("2d");[m
[32m+[m
[32m+[m[32m        pincel.font="15px Georgia";[m
[32m+[m[32m        pincel.fillStyle="black";[m
[32m+[m[32m        pincel.fillText(texto, x, y);[m[41m    [m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    function dibujarBarra(x, y, serie, colores, texto) {[m
[32m+[m
[32m+[m[32m        escribirTexto(x, y - 10, texto);[m
[32m+[m
[32m+[m[32m        var sumaAltura = 0;[m
[32m+[m[32m        for (var i = 0; i < serie.length; i++) {[m
[32m+[m[32m            var altura = serie[i];[m
[32m+[m[32m            dibujarRectangulo(x, y + sumaAltura, 50, altura, colores[i]);[m
[32m+[m[32m            sumaAltura = sumaAltura + altura;[m
[32m+[m[32m        }[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    var colores = ["blue","green","yellow", "red","gray"];[m
[32m+[m[32m    var serie2009 = [6,47,41,3,3];[m
[32m+[m[32m    var serie2019 = [81,9,3,3,4];[m
[32m+[m
[32m+[m[32m    dibujarBarra(50, 50, serie2009, colores, "2009");[m
[32m+[m[32m    dibujarBarra(150, 50, serie2019, colores, "2019");[m
[32m+[m
[32m+[m[32m</script>[m
[32m+[m
[32m+[m
