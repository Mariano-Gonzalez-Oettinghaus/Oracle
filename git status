[32m+[m[32m    pincel.fillRect(0,0,600,400);[m[41m [m
[32m+[m[32m    var colores = ["blue", "red", "green"];[m
[32m+[m[32m    var indiceColorActual = 0; // comienza con blue[m
[32m+[m
[32m+[m[32m    function dibujarCirculo(evento){[m
[32m+[m[32m        var x = evento.pageX - pantalla.offsetLeft;[m
[32m+[m[32m        var y = evento.pageY - pantalla.offsetTop;[m[41m    [m
[32m+[m[32m        pincel.fillStyle = colores[indiceColorActual];[m
[32m+[m[32m        pincel.beginPath();[m
[32m+[m[32m        pincel.arc(x,y,10,0,2*3.14);[m
[32m+[m[32m        pincel.fill();[m
[32m+[m[32m        console.log(x + "," + y);[m[41m        [m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    pantalla.onclick = dibujarCirculo;[m
[32m+[m
[32m+[m[32m    function alterarColor() {[m
[32m+[m[32m        indiceColorActual++;[m
[32m+[m[32m        if(indiceColorActual>= colores.length) {[m
[32m+[m[32m             indiceColorActual = 0; //vuelve para el primer color, blue[m
[32m+[m[32m        }[m
[32m+[m[32m        return false; //men√∫ contextual padr√≥n de `canvas` no sea exhibido[m
[32m+[m[32m    }[m
[32m+[m
[32m+[m[32m    pantalla.oncontextmenu = alterarColor;[m
[32m+[m
[32m+[m[32m</script>[m[41m [m
